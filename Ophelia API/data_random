import random
from datetime import datetime

def get_data_TV (id_estado):
    if id_estado == 1:
        value= random.randint(60,80)
        fecha = str(datetime.today())
        return value, fecha
    elif id_estado == 0:
        value= 0
        fecha = str(datetime.today())
        return value
    elif id_estado == 2:
        value= random.randint(7,13)
        fecha = str(datetime.today())
        return value

def get_lamp1 (id_estado):
    if id_estado == 1:
        value= random.randint (90,105)
        return value
    elif id_estado == 0:
        value= 0
        return value
    elif id_estado == 2:
        value= random.randint (7,13)
        return value

